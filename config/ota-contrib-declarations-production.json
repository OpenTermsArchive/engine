{
  "services": {
    "declarationsPath": "../ota-contrib-declarations/declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../ota-contrib-versions",
          "publish": true,
          "prefixMessageToSnapshotId": "This version was recorded after filtering snapshot with Mongo ID "
        }
      }
    },
    "snapshots": {
      "storage": {
        "type": "mongo"
      }
    }
  },
  "logger": {
    "sendMailOnError": {
      "to": "admin@opentermsarchive.org",
      "from": "noreply@opentermsarchive.org",
      "sendWarnings": false
    }
  },
  "notifier": {
    "sendInBlue": {
      "updatesListId": 191,
      "updateTemplateId": 21
    }
  },
  "tracker": {
    "githubIssues": {
      "repository": "OpenTermsArchive/contrib-declarations"
    }
  },
  "dataset": {
    "servicesRepositoryName": "contrib-declarations",
    "versionsRepositoryURL": "https://github.com/OpenTermsArchive/contrib-versions"
  }
}
