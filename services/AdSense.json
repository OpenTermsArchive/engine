{
  "name": "AdSense",
  "documents": {
    "Terms of Service": {
      "fetch": "https://www.google.com/adsense/tc/2018/UnitedKingdom.html",
      "select": "body"
    },
    "Program Policies": {
      "fetch": "https://support.google.com/adsense/answer/48182",
      "select": ".article-container",
      "remove": ".action-button, .zippy"
    },
    "Parent Organization Privacy Policy": {
      "fetch": "https://policies.google.com/privacy?hl=en-GB",
      "filter": [
        "removeUTMfromUrls"
      ],
      "select": "div[role=article]",
      "remove": ".KMMDve, .TL2alc, img[src$='.svg']"
    },
    "Brand Guidelines": {
      "fetch": "https://support.google.com/adsense/answer/6370057",
      "select": ".article-container",
      "remove": ".no-bullets"
    },
    "Quality Guidelines": {
      "fetch": "https://support.google.com/webmasters/answer/35769",
      "select": ".article-container"
    },
    "Acceptable Use Policy": {
      "fetch": "https://support.google.com/adsense/answer/9335564?hl=en&ref_topic=1250104",
      "select": ".article-container"
    },
    "Restricted Use Policy": {
      "fetch": "https://support.google.com/adsense/answer/9335567?hl=en&ref_topic=1250104",
      "select": ".article-container"
    },
    "Data Controller Agreement": {
      "fetch": "https://privacy.google.com/businesses/controllerterms/",
      "select": ".ads-controller"
    },
    "Trackers Policy": {
      "fetch": "https://policies.google.com/technologies/ads",
      "select": "div[role=article]"
    },
    "User Consent Policy": {
      "fetch": "https://www.google.com/about/company/user-consent-policy/",
      "select": ".page-section"
    }
  }
}
