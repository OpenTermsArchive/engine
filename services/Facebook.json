{
  "name": "Facebook",
  "documents": {
    "Terms of Service": {
      "fetch": "https://www.facebook.com/legal/terms/plain_text_terms",
      "filter": [
        "cleanUrls"
      ],
      "select": ".fb_content"
    },
    "Privacy Policy": {
      "fetch": "https://www.facebook.com/about/privacy",
      "filter": [
        "removeReturnToTopButtons",
        "cleanUrls"
      ],
      "select": "._5tkp"
    },
    "Developer Policy": {
      "fetch": "https://developers.facebook.com/policy",
      "filter": [
        "removeHelpButtons",
        "cleanUrls"
      ],
      "select": "._4bl9"
    },
    "Cookies Policy": {
      "fetch": "https://www.facebook.com/policies/cookies",
      "filter": [
        "removeImgFromCookiesPolicy",
        "removeReturnToTopButtons",
        "cleanUrls"
      ],
      "select": "._5tkp"
    },
    "Commercial Terms": {
      "fetch": "https://www.facebook.com/legal/commercial_terms",
      "filter": [
        "numberListCorrectly",
        "removeHelpButtons",
        "cleanUrls"
      ],
      "select": "#content"
    }
  }
}
