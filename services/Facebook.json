{
  "name": "Facebook",
  "documents": {
    "Terms of Service": {
      "fetch": "https://www.facebook.com/legal/terms/plain_text_terms",
      "filter": ["cleanUrls"],
      "select": ".fb_content"
    },
    "Privacy Policy": {
      "fetch": "https://www.facebook.com/about/privacy",
      "filter": ["removeReturnToTopButtons", "cleanUrls"],
      "select": "._5tkp"
    },
    "Developer Terms": {
      "fetch": "https://developers.facebook.com/policy",
      "filter": ["removeHelpButtons", "cleanUrls"],
      "select": "._4bl9"
    },
    "Trackers Policy": {
      "fetch": "https://www.facebook.com/policies/cookies",
      "filter": ["removeImgFromCookiesPolicy", "removeReturnToTopButtons", "cleanUrls"],
      "select": "._5tkp"
    },
    "Commercial Terms": {
      "fetch": "https://www.facebook.com/legal/commercial_terms",
      "filter": ["numberListCorrectly", "removeHelpButtons", "cleanUrls"],
      "select": "#content"
    },
    "Data Processor Agreement": {
      "fetch": "https://www.facebook.com/legal/EU_data_transfer_addendum",
      "filter": ["numberListCorrectly", "cleanUrls"],
      "select": "#content"
    },
    "Live Policy": {
      "fetch": "https://en-gb.facebook.com/policies/live",
      "select": "#content"
    },
    "Law Enforcement Guidelines": {
      "fetch": "https://www.facebook.com/safety/groups/law/guidelines",
      "filter": ["cleanUrls"],
      "select": ["._4xy4 > div"],
      "remove": [".img"]
    }
  }
}
