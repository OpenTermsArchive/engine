{
  "name": "Yahoo",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/yahoo.de.xml",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://policies.yahoo.com/ie/en/yahoo/privacy/index.htm?redirect=no",
      "select": ".l-content-well-content"
    },
    "Terms of Service": {
      "fetch": "https://policies.yahoo.com/ie/en/yahoo/terms/utos/index.htm?redirect=no",
      "select": ".l-content-well-content"
    },
    "Ad Publishing Policy": {
      "fetch": "https://policies.yahoo.com/us/en/yahoo/terms/advertising/publisher/index.htm",
      "select": [".yui3-g:nth-child(2) > .yui3-u-2-3 > .yui3-by-content", "h1"]
    }
  }
}
