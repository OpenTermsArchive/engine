{
  "name": "Google Analytics",
  "documents": {
    "Terms of Service": {
      "fetch": "https://marketingplatform.google.com/about/analytics/terms/gb/",
      "select": ".terms-section"
    },
    "Parent Organization Privacy Policy": {
      "fetch": "https://policies.google.com/privacy?hl=en-GB",
      "filter": [
        "removeUTMfromUrls"
      ],
      "select": "div[role=article]",
      "remove": ".KMMDve, .TL2alc, img[src$='.svg']"
    },
    "Data Processor Agreement": {
      "fetch": "https://privacy.google.com/businesses/processorterms/",
      "select": ".ads-controller"
    },
    "Trackers Policy": {
      "fetch": "https://policies.google.com/technologies/ads",
      "select": "div[role=article]"
    },
    "User Consent Policy": {
      "fetch": "https://www.google.com/about/company/user-consent-policy/",
      "select": ".page-section"
    },
    "Data Controller Agreement": {
      "fetch": "https://support.google.com/analytics/answer/9012600",
      "select": ".article-container"
    }
  }
}
