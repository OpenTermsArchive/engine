{
  "name": "Gmail",
  "importedFrom": "https://github.com/tosdr/tosback2/blob/f762cd4bbb2571272985fed009201a23300ba9b2/rules/gmail.com.xml",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://policies.google.com/privacy?hl=en-GB",
      "filter": ["removeUTMfromUrls"],
      "select": "div[role=article]",
      "remove": ".KMMDve, .TL2alc, .aYyAOe, img[src$='.svg'], img[src^='data:image/svg+xml']"
    },
    "Terms of Service": {
      "fetch": "https://policies.google.com/terms?hl=en-GB",
      "filter": ["removeUTMfromUrls", "removeCountryVersion"],
      "select": "div[role=article]",
      "remove": ".bVzSHe + p, .RwC1f, img[src$='.svg'], img[src^='data:image/svg+xml']"
    },
    "Acceptable Use Policy": {
      "fetch": "https://www.google.com/gmail/about/policy/?hl=en_GB",
      "select": ".policy__content"
    }
  }
}
