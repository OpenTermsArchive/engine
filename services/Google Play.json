{
  "name": "Google Play",
  "documents": {
    "Developer Terms": {
      "fetch": "https://play.google.com/intl/ALL_en/about/developer-distribution-agreement.html",
      "select": "body > .play-contained"
    },
    "Terms of Service": {
      "fetch": "https://play.google.com/intl/en_US/about/play-terms/index.html",
      "select": "body > .play-contained"
    },
    "Brand Guidelines": {
      "fetch": "https://developer.android.com/distribute/marketing-tools/brand-guidelines",
      "filter": [
        "downloadImages"
      ],
      "select": "article.devsite-article-inner"
    },
    "Review Guidelines": {
      "fetch": "https://support.google.com/googleplay/android-developer/answer/9904549?hl=en",
      "select": "article.page"
    }
  }
}
